# Shared LaunchDarkly Configuration
LD_SERVER_KEY='your-launchdarkly-server-key'

# LaunchDarkly project key
LD_PROJECT_KEY='your-project-key'

# Shared AI Config identifier for multi-agent system
LD_AI_CONFIG_ID='multi-agent-llm-prompt'
LD_AI_CONFIG_ID_TEACHER='teacher-orchestrator'
LD_AI_CONFIG_ID_ENGLISH='english-assistant'
LD_AI_CONFIG_ID_COMPUTER_SCIENCE='computer-science-assistant'
LD_AI_CONFIG_ID_MATH='math-assistant'
LD_AI_CONFIG_ID_LANGUAGE='language-assistant'
LD_AI_CONFIG_ID_GENERAL='general-assistant'

# Agent-specific roles for context targeting
TEACHER_ORCHESTRATOR_ROLE=teacher-orchestrator
COMPUTER_SCIENCE_ASSISTANT_ROLE=computer-science-assistant
MATH_ASSISTANT_ROLE=math-assistant
LANGUAGE_ASSISTANT_ROLE=language-assistant
ENGLISH_ASSISTANT_ROLE=english-assistant
GENERAL_ASSISTANT_ROLE=general-assistant

# MongoDB Configuration
MONGODB_CONNECTION_STRING=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&appName=YourApp
MONGODB_DATABASE=memory_demo
MONGODB_COLLECTION=memory
MONGODB_COLLECTION_CONVERSATION=conversation_memory
MONGODB_COLLECTION_WORKFLOW=workflow_memory
MONGODB_COLLECTION_LONG_TERM=long_term_memory

# Memory Configuration
WORKFLOW_MEMORY_ENABLED=true
LONG_TERM_MEMORY_ENABLED=true
MEMORY_RETENTION_DAYS=30
MAX_MEMORY_CONTEXT_SIZE=5000
MAX_INTERACTIONS_PER_SESSION=10

# Optional: Additional configuration parameters
# LD_ENVIRONMENT=production
# LD_TIMEOUT=5000
# LD_DEBUG=false

# Agent Configuration
AGENT_NAME=MultiAgentSystem
AGENT_VERSION=1.0.0

# Logging Configuration
LOG_LEVEL=INFO

# Arize AI Configuration
ARIZE_SPACE_ID="your-arize-space-id"
ARIZE_API_KEY="your-arize-api-key"
OTEL_EXPORTER_OTLP_ENDPOINT="https://otlp.arize.com/v1"
